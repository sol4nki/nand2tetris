/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */

// `Y = not(sel)​​.I0​ + sel​.I1`​

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel , out=notsel );
    And(a=notsel , b=a , out=and1 );
    And(a=sel , b=b , out=and2 );
    Or(a=and1 , b=and2 , out=out );

}